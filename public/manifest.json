{
  "manifest_version": 3,
  "name": "AI README Generate",
  "version": "1.1",
  "description": "Generate README.md file",
  "permissions": ["tabs", "storage"],  
  "action": {
    "default_title": "AI README Maker",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
